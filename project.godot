config_version=5

[application]

config/name="ReloadedK's Godot Fangame Engine"
config/tags=PackedStringArray("fangame_engine")
run/main_scene="res://Rooms/00_Title_and_menus/rMainMenu.tscn"
config/features=PackedStringArray("4.1", "Mobile")
boot_splash/bg_color=Color(0.141176, 0.141176, 0.141176, 1)
boot_splash/show_image=false
config/icon="res://Resources/Project/icon.png"

[audio]

buses/default_bus_layout="res://Resources/Project/default_bus_layout.tres"

[autoload]

GLOBAL_SAVELOAD="*res://Scripts/Globals/scrGlobalSaveload.gd"
GLOBAL_SETTINGS="*res://Scripts/Globals/scrGlobalSettings.gd"
GLOBAL_GAME="*res://Scripts/Globals/scrGlobalGame.gd"
GLOBAL_INSTANCES="*res://Scripts/Globals/scrGlobalInstances.gd"
GLOBAL_PARTICLES="*res://Scripts/Globals/scrGlobalParticles.gd"
GLOBAL_MUSIC="*res://Scripts/Globals/scrGlobalMusicPlayer.gd"
GLOBAL_SOUNDS="*res://Objects/System/objSoundManager.tscn"
objHUD="*res://Objects/System/objHUD.tscn"
objWarpTransition="*res://Objects/System/objWarpTransition.tscn"

[debug]

file_logging/enable_file_logging.pc=false

[display]

window/size/viewport_width=800
window/size/viewport_height=608
window/size/resizable=false
window/stretch/mode="viewport"

[filesystem]

import/blender/enabled=false

